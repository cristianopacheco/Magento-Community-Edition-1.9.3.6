<?xml version="1.0"?>
<config>
    <modules>
        <MagedIn_Rewrites>
            <version>0.1.0</version>
        </MagedIn_Rewrites>
    </modules>
    <global>
        <models>
            <magedin_rewrites>
                <class>MagedIn_Rewrites_Model</class>
            </magedin_rewrites>
            <customer>
                <rewrite>
                    <customer>MagedIn_Rewrites_Model_Customer_Customer</customer>
                </rewrite>
            </customer>
        </models>
        <helpers>
            <magedin_rewrites>
                <class>MagedIn_Rewrites_Helper</class>
            </magedin_rewrites>
            <customer>
                <rewrite>
                    <data>MagedIn_Rewrites_Helper_Customer_Data</data>
                </rewrite>
            </customer>
        </helpers>
        <blocks>
            <magedin_rewrites>
                <class>MagedIn_Rewrites_Block</class>
            </magedin_rewrites>
            <page>
                <rewrite>
                    <html_header>MagedIn_Rewrites_Block_Page_Html_Header</html_header>
                </rewrite>
            </page>
        </blocks>
    </global>
    <frontend>
        <routers>
            <magedin_rewrites>
                <use>standard</use>
                <args>
                    <module>MagedIn_Rewrites</module>
                    <frontName>rewrites</frontName>
                </args>
            </magedin_rewrites>
        </routers>
    </frontend>
</config>
